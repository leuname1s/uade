    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nro1" type="Integer" array="False" size=""/>
            <declare name="nro2" type="Integer" array="False" size=""/>
            <declare name="prom" type="Integer" array="False" size=""/>
