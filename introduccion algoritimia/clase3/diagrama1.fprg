                    <output expression="&quot;recursa&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="prom &gt;= 8">
                        <then>
                            <output expression="&quot;promociona&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;rinde final&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="&quot;fin del programa&quot;" newline="True"/>
        </body>
